{
  "author": "Robert Smieja",
  "dependencies": {
    "@robbot/logging-filter": "workspace:0.0.7",
    "@robbot/per-server-event-handlers": "workspace:0.0.7",
    "@robbot/puppeteer-pdf": "workspace:0.0.5",
    "@robbot/robbot-core": "workspace:^0.0.13",
    "axios": "^0.21.0",
    "discord.js": "^12.5.1",
    "get-stream": "^6.0.0",
    "uuid": "^8.3.2"
  },
  "description": "RobBot, a Discord bot",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "@types/puppeteer": "^5.4.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ]
    }
  },
  "keywords": [
    "discord",
    "discordjs",
    "robbot",
    "robbot-core",
    "robbot-discord"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,md}": [
      "prettier --write"
    ],
    "src/*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ]
  },
  "main": "dist/index.js",
  "name": "@robbot/robbot",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/robbot-discord/robbot",
  "scripts": {
    "build": "npm-run-all --parallel build:js lint",
    "build:js": "tsc",
    "clean": "rimraf dist/",
    "lint": "eslint ./src/**/*",
    "lint:fix": "eslint --fix ./src/**/*",
    "lint:staged": "lint-staged",
    "prepublishOnly": "npm-run-all --serial clean build --parallel test lint",
    "prettier": "prettier --write \"./**/*.{js,ts,jsx,tsx,json,md}\"",
    "start": "node dist/index.js",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "version": "npm run lint:fix && git add -A src"
  },
  "version": "0.0.0"
}
